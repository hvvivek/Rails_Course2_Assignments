<h1>Login Page</h1>
<%= form_for(:user, url: sessions_path) do |f| %>
    <div class="field">
        <%= f.label :username %>
        <%= f.text_field :username%>
    </div> 
    <div class="field">
        <%= f.label :password %>
        <%= f.text_field :password%>
    </div>
    <div class="actions">
        <%= f.submit "Login" %>
    </div>
<% end %>